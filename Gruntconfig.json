{
  "srcPaths": {
    "make": "src/project.make",
    "drupal": "src"
  },
  "buildPaths": {
    "build": "build",
    "html": "build/html",
    "package": "build/packages",
    "reports": "build/reports",
    "temp": "build/temp",
    "profile": "src/profiles",
    "custom_theme": "src/themes/custom/geek_portland",
    "patternlab": "src/themes/custom/geek_portland/pattern-lab",
    "modules": "build/html/sites/all/modules",
    "themes": "build/html/sites/all/themes",
    "libraries": "build/html/sites/all/libraries"
  },
  "domain": "geek-portland.dev",
  "packages": {
    "srcFiles": ["!sites/*/files/**", "!xmlrpc.php", "!modules/php/*"],
    "projFiles": ["README*", "bin/**"]
  },
  "phpcs": {
    "path": "vendor/bin/phpcs"
  },
  "phpmd": {
    "path": "vendor/bin/phpmd"
  },
  "drush": {
    "cmd": "vendor/bin/drush"
  },
  "behat": {
    "flags": "--tags ~@wip"
  },
  "eslint": true
}
